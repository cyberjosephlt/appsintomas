<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_color"
    tools:context=".activity_alumno">

    <TextView
        android:id="@+id/lbltituloalumno"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="132dp"
        android:layout_marginTop="16dp"
        android:text="BIENVENIDO"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lbltituloalumno2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="22dp"
        android:text="Steve Jobs"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imagenalumno"
        app:layout_constraintTop_toBottomOf="@+id/lbltituloalumno" />

    <TextView
        android:id="@+id/lbltituloalumno3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="61dp"
        android:layout_marginTop="222dp"
        android:layout_marginEnd="61dp"
        android:text="ESTADO:HABILITADO"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/button4"
        app:layout_constraintStart_toStartOf="@+id/button3"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imagenalumno"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="23dp"
        app:layout_constraintBottom_toTopOf="@+id/lbltituloalumno3"
        app:layout_constraintEnd_toEndOf="@+id/lbltituloalumno3"
        app:layout_constraintStart_toStartOf="@+id/lbltituloalumno3"
        app:layout_constraintTop_toTopOf="@+id/lbltituloalumno2"
        app:srcCompat="@drawable/avatar1" />

    <ListView
        android:id="@+id/listasintomasal"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="31dp"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lbltituloalumno3" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="49dp"
        android:layout_marginBottom="79dp"
        android:text="GUARDAR"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="74dp"
        android:onClick="llamar5"
        android:text="HISTORIAL"
        app:layout_constraintEnd_toEndOf="@+id/listasintomasal"
        app:layout_constraintTop_toBottomOf="@+id/listasintomasal" />
</androidx.constraintlayout.widget.ConstraintLayout>